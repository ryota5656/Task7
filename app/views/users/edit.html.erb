
<%= form_with model: @user , local:true do |f| %>
  <table>
    <tr>
      <th><%= f.label :name ,"タイトル" %></th>
      <td><%= f.text_field :name %></td>
    </tr>

    <tr>
      <th><%= f.label :introduction ,"開始日" %></th>
      <td><%= f.text_field:introduction %></td>
    </tr>
    
    <tr>
      <th><%= f.label :email ,"開始日" %></th>
      <td><%= f.text_field:email %></td>
    </tr>
  </table>
    <ul>
      <li><%= f.submit "スケジュールを新規登録する"%></li>
      <li><%= link_to "スケジュール一覧に戻る", user_path(id: current_user.id) %></li>
    </ul>      
  
  <% end %>

